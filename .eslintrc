{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/prop-types": 0,
    "react/jsx-boolean-value":0,
    "react/jsx-quotes": 0,
    "react/no-multi-comp":0,
    "jsx-quotes": 2,
    "id-length":0,
    "func-names":0,
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEVELOPMENT__": true,
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DEVTOOLS__": true,
    "socket": true,
    "io": true
  }
}
